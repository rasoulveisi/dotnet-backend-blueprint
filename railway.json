{
  "build": {
    "builder": "NIXPACKS",
    "nixpacks": {
      "providers": ["dotnet"],
      "phases": {
        "setup": {
          "nixPkgs": ["dotnet-sdk_9"]
        }
      }
    }
  },
  "deploy": {
    "startCommand": "dotnet run --project src/TemplateApp.Api --urls=http://0.0.0.0:$PORT"
  }
}